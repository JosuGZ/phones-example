  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && webpack --config webpack.config.prod.js && sass src/styles/app.scss build/dist/style.css --no-source-map --style=compressed && cp -r images build/dist/images && cp index.html build/dist/index.html",
    "dev": "concurrently \"ts-node-dev server/server.ts\" \"webpack-dev-server\" \"sass src/styles/app.scss style.css --watch --no-stop-on-error\"",
    "test": "jest --watch",
    "start": "node ./build/server.js",
    "build:docker": "npm run build && docker build -t example/phones-example .",
    "start:docker": "docker run --rm -d -p 3000:3000 --name phones-example example/phones-example"
  },
